{
  "type": "module",
  "name": "domus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
    "lint": "bunx prettier --plugin-search-dir domus --check . && eslint .",
    "format": "bunx prettier --plugin-search-dir domus --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-svelte-csf": "^4.0.9",
    "@storybook/blocks": "^7.4.5",
    "@storybook/builder-vite": "^7.4.5",
    "@storybook/svelte": "^7.4.5",
    "@storybook/sveltekit": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@tsconfig/strictest": "^2.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@unocss/svelte-scoped": "^0.56.5",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.7.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-svelte": "^2.30.0",
    "lightningcss": "^1.21.8",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "storybook": "^7.4.5",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unocss": "^0.56.5",
    "vite": "^4.4.2",
    "vitest": "^0.34.4",
    "unocss-preset-daisy": "^6.0.1"
  }
}
